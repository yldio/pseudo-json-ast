{
  "sourceMaps": "inline",
  "presets": [
    ["env", {
      "spec": true,
      "modules": false,
      "targets": {
        "browsers": [
          "last 2 versions",
          "safari >= 7"
        ],
        "node": 6
      }
    }]
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "spec": true,
          "targets": {
            "node": "current"
          }
        }]
      ],
      "plugins": [
        "istanbul"
      ]
    }
  }
}